{
  "name": "Speedio - Linkedin",
  "description": "Busque os dados da empresa desse perfil ou a empresa em que ela trabalha",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["background", "activeTab", "scripting", "notifications", "alarms", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/icons/16.png",
      "32": "/icons/32.png",
      "48": "/icons/48.png",
      "128": "/icons/128.png"
    }
  },
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  }
}